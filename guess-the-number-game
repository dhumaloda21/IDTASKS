import random

def Guess_number():
  secret_number = random.randint(0,5)
  max_attempt = 5
  attempt = 0

  guess = int(input("enter number:"))

  while attempt < max_attempt:
    try :
      guess = int(input("enter number:"))

      if guess == secret_number:
        print("correct ")
        break
      else:
        print("incorrect")
        break

      attempt += 1

    except ValueError:
      print("Invalid input. Please enter a valid integer.")


Guess_number()
